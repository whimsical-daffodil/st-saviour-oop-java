class Chaucer extends BritishLit { 

    private String poet;
    private boolean London;

    public Chaucer(boolean english, String title, String author, String poet, boolean London){
        super(english, title, author);
        this.poet = poet;
        this.London = London;

    }

    // getter and setter
    public String getPoet(){
        return poet;
    }

    public void setPost(String poet){
        this.poet = poet;
    }

    public boolean isLondon(){
        return London;
    }

    public void setLondon(boolean London){
        this.London = London
    }

    @Override
    public void wrote(){
        system.out.println(author "wrote" + title + "in the year" + published);
    }
}

    

