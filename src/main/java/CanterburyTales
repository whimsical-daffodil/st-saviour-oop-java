class CanterburyTales extends Chaucer{ 
    public boolean rhymes;
    public String characterName;

    public CanterburyTales(boolean english, String title, String author, String poet, boolean London, boolean rhymes, String characterName){
        super(english, title, author, poet, London);
        this.rhymes = rhymes;
        this.characterName = characterName;
    }
    
    public void into(){
        system.out.println(characterName + "is a character from" + title + "written by" + author);
    }
}
