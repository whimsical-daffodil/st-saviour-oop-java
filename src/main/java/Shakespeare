class Shakespeare extends BritishLit {

    public int published; 
    public boolean aprilBaby;

    // constructor 
    public Shakespeare (String title, String author, boolean english, int published, boolean aprilBaby) {
        super(title, english, author);
        this.published = published
        this.aprilBaby = aprilBaby
    }
    
    @Override
    public void wrote(){
        system.out.println("Shakespeare wrote" + title + "in the year" + published);
    }
      
}