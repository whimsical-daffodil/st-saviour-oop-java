class Shakespeare extends BritishLit {

    public int published; 
    public boolean aprilBaby;

    // constructor 
    public Shakespeare (boolean english, String title, String author, int published, boolean aprilBaby) {
        super(title, english, author);
        this.published = published;
        this.aprilBaby = aprilBaby;
    }
    
    @Override
    public void wrote(){
        system.out.println(author "wrote" + title + "in the year" + published);
    }
      
}